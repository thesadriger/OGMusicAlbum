{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {"type": "grafana", "id": "grafana", "name": "Grafana", "version": "11.0.0"},
    {"type": "panel", "id": "timeseries", "name": "Time series", "version": ""},
    {"type": "panel", "id": "stat", "name": "Stat", "version": ""},
    {"type": "panel", "id": "gauge", "name": "Gauge", "version": ""},
    {"type": "panel", "id": "table", "name": "Table", "version": ""},
    {"type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.0.0"}
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "row",
      "title": "Overview",
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "panels": []
    },
    {
      "type": "stat",
      "title": "‚è±Ô∏è API Uptime",
      "id": 1,
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 1},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "max(ogma_uptime_seconds{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "{{instance}}",
          "interval": ""
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "auto",
        "text": {},
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{"color": "red", "value": null}, {"color": "green", "value": 1}]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "üì¶ Indexer Lag (min)",
      "id": 2,
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "max(ogma_indexer_lag_min{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "{{instance}}",
          "interval": ""
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "values": false},
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 3},
              {"color": "red", "value": 10}
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "ü§ñ Bot Lag (min)",
      "id": 3,
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 1},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "max(ogma_bot_lag_min{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "{{instance}}"
        }
      ],
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 3},
              {"color": "red", "value": 10}
            ]
          }
        }
      }
    },
    {
      "type": "gauge",
      "title": "Targets UP (ogma job)",
      "id": 4,
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 1},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum(up{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "up"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "reduceOptions": {"calcs": ["lastNotNull"]},
        "orientation": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 5,
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "green", "value": 2}
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Indexer & Bot Lag over time (min)",
      "id": 10,
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 5},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "ogma_indexer_lag_min{job=~\"$job\", instance=~\"$instance\"}",
          "legendFormat": "indexer {{instance}}"
        },
        {
          "refId": "B",
          "expr": "ogma_bot_lag_min{job=~\"$job\", instance=~\"$instance\"}",
          "legendFormat": "bot {{instance}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 3},
              {"color": "red", "value": 10}
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "right"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    },
    {
      "type": "row",
      "title": "Runtime (process / Python)",
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 13}
    },
    {
      "type": "timeseries",
      "title": "CPU (cores) ‚Äî process",
      "id": 20,
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 14},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (instance) (rate(process_cpu_seconds_total{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "cores"}}
    },
    {
      "type": "timeseries",
      "title": "Memory RSS",
      "id": 21,
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 14},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "max by (instance) (process_resident_memory_bytes{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "bytes"}}
    },
    {
      "type": "timeseries",
      "title": "Open FDs",
      "id": 22,
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 14},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "max by (instance) (process_open_fds{job=~\"$job\", instance=~\"$instance\"})",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "none"}}
    },
    {
      "type": "timeseries",
      "title": "Python GC objects collected (rate)",
      "id": 23,
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 20},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (generation, instance) (rate(python_gc_objects_collected_total{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "gen {{generation}} ‚Äî {{instance}}"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "ops"}}
    },
    {
      "type": "timeseries",
      "title": "Python GC time (rate)",
      "id": 24,
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 20},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (instance) (rate(python_gc_duration_seconds_sum{job=~\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "s"}}
    },
    {
      "type": "row",
      "title": "Scrape Health",
      "collapsed": false,
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 26}
    },
    {
      "type": "table",
      "title": "Targets status (up)",
      "id": 30,
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 27},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "up{job=~\"$job\", instance=~\"$instance\"}",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {"type": "value", "options": {"0": {"index": 0, "text": "DOWN", "color": "red"}, "1": {"index": 1, "text": "UP", "color": "green"}}}
          ],
          "custom": {"align": "auto"}
        },
        "overrides": []
      },
      "options": {"showHeader": true}
    },
    {
      "type": "timeseries",
      "title": "Scrape duration (s)",
      "id": 31,
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 27},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [
        {
          "refId": "A",
          "expr": "scrape_duration_seconds{job=~\"$job\", instance=~\"$instance\"}",
          "legendFormat": "{{instance}}"
        }
      ],
      "fieldConfig": {"defaults": {"unit": "s"}}
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["ogma", "prometheus", "python", "fastapi"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "current": {"selected": true, "text": "Prometheus", "value": "${DS_PROMETHEUS}"},
        "refresh": 1
      },
      {
        "name": "job",
        "type": "query",
        "label": "Job",
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "query": "label_values(up, job)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {"selected": true, "text": "ogma", "value": "ogma"}
      },
      {
        "name": "instance",
        "type": "query",
        "label": "Instance",
        "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
        "query": "label_values(up{job=~\"$job\"}, instance)",
        "refresh": 2,
        "includeAll": true,
        "multi": true,
        "current": {"selected": true, "text": "All", "value": ".*"}
      }
    ]
  },
  "time": {"from": "now-24h", "to": "now"},
  "timepicker": {"refresh_intervals": ["5s","10s","30s","1m","5m","15m","1h"]},
  "timezone": "",
  "title": "OGMA ‚Äî Production Monitoring",
  "uid": null,
  "version": 1,
  "weekStart": ""
}